<Window x:Class="Surveyval.NewQuestionnaire"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Surveyval"
        mc:Ignorable="d" Height="160.797" Width="300.39" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="labelName" Content="" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxName" Height="22" Margin="10,41,10,0" TextWrapping="Wrap" VerticalAlignment="Top" TextChanged="TextBoxName_TextChanged"/>
        <Button x:Name="buttonSave" Content="" HorizontalAlignment="Left" Margin="10,0,0,10" Width="74" Height="20" VerticalAlignment="Bottom" IsEnabled="False" Click="ButtonSave_Click"/>
        <Button x:Name="buttonCancel" Content="" Margin="0,0,10,10" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="76" Click="ButtonCancel_Click"/>

    </Grid>
</Window>
